<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_stock_config_settings" model="ir.ui.view">
            <field name="name">stock settings</field>
            <field name="model">res.config.settings</field>
            <field name="inherit_id" ref="stock.res_config_settings_view_form"/>
            <field name="arch" type="xml">
                <div name="warehouse_setting_container" position="inside">
                    <div class="col-12 col-lg-6 o_setting_box">
                        <div class="o_setting_right_pane">
                            <div class="content-group">
                                <label for="label_printer_url" class="o_form_label"/>  <field name="label_printer_url" class="o_light_label" style="width: 200px;"/>
                            </div>
                        </div>
                    </div>
                </div>
            </field>
        </record>
    </data>
</odoo>